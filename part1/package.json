{
  "name": "simple-express",
  "version": "1.0.0",
  "description": "express server with babel installed",
  "main": "index.js",
  "scripts": {
    "start": "nodemon lib/index.js --exec babel-node",
    "build": "babel lib -d dist",
    "serve": "node dist/index.js",
    "predeploy": "npm run build",
    "deploy": "eb deploy"
  },
  "keywords": [],
  "license": "MIT",
  "engines": {
    "node": "12.18.4"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "mysql2": "^2.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "nodemon": "^1.17.5"
  }
}
